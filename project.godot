config_version=5

[application]

config/name="UberGodotGameJamKit"
run/main_scene="res://Level/Splash.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://icon.svg"
config/windows_native_icon="res://godot_icon.ico"

[audio]

buses/default_bus_layout="res://addons/StandardAssets/Default/default_bus_layout.tres"

[autoload]

Demo_Game_Manager="*res://Prefab/Demo_Game_Manager.tscn"

[dotnet]

project/assembly_name="UberGodotGameJamKit"

[editor_plugins]

enabled=PackedStringArray("res://addons/EditorToolbox/plugin.cfg")

[file_customization]

folder_colors={
"res://Level/": "green",
"res://Material/": "orange",
"res://Prefab/": "blue",
"res://Todo/": "red",
"res://addons/StandardAssets/Material/": "orange",
"res://addons/StandardAssets/Prefab/": "blue"
}

[filesystem]

import/blender/enabled=false

[importer_defaults]

